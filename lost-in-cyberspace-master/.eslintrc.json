{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "semi": [ "error", "always" ],
      "no-console": "off",
      "no-global-assign": "warn",
      "no-sparse-arrays": "off",
      "eqeqeq": "error"
    }
}
